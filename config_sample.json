{
    "constants":
    [
        {"rTor":0.5},{"RTor":1.5}
    ],

    "functions":
    [
        {
            "name":"squad",
            "args":["x"],
            "body":"x*x"
        },
        {
            "name":"qube",
            "args":["x"],
            "body":"x*x*x"
        },
        {
            "name":"Himmelblau",
            "args":["x","y"],
            "body":"squad(x*x+y-11)+squad(x+y*y-7)/400"
        },
        {
            "name":"Rosenbrock",
            "args":["x","y"],
            "body":"(squad(1-x)+10*squad(y-x*x))/20"
        },
        {
            "name":"xTor",
            "args":["s","t","time"],
            "body":"cos (s)*(RTor+rTor*cos(t))*cos(time)+rTor*sin(t)*sin(time)"
        },
        {
            "name":"yTor",
            "args":["s","t","time"],
            "body":"sin(s)*(RTor+rTor*cos(t))"
        },
        {
            "name":"zTor",
            "args":["s","t","time"],
            "body":"rTor*sin(t)*cos(time)-cos(s)*(RTor+rTor*cos(t))*sin(time)"
        },
        {
            "name":"xMebius",
            "args":["s","t"],
            "body":"cos(s)*(1+t/2*cos(s/2))"
        },
        {
            "name":"yMebius",
            "args":["s","t"],
            "body":"sin(s)*(1+t/2*cos(s/2))"
        },
        {
            "name":"zMebius",
            "args":["s","t"],
            "body":"sin(s/2)*t/2"
        },
        {
            "name":"xHelic",
            "args":["s","t"],
            "body":"s*cos(t)"
        },
        {
            "name":"yHelic",
            "args":["s","t"],
            "body":"s*sin(t)"
        },
        {
            "name":"zHelic",
            "args":["s","t"],
            "body":"t"
        }

    ]
}
